# This rosinstall contains all uos-ros-pkg stacks for ROS Hydro that are "dry" (i.e., based on rosbuild). It uses
# public (i.e. anonymous) git URLs, so no SSH keys are required. It is
# referenced from http://www.ros.org/wiki/uos-ros-pkg.

- git: {local-name: uos-ros-pkg/calvin_robot, uri: 'https://github.com/uos/calvin_robot', version: hydro}
- git: {local-name: uos-ros-pkg/diffdrive_gazebo_plugin, uri: 'https://github.com/uos/diffdrive_gazebo_plugin', version: hydro}
- git: {local-name: uos-ros-pkg/katana_driver, uri: 'https://github.com/uos/katana_driver', version: hydro}
- git: {local-name: uos-ros-pkg/kurt_driver, uri: 'https://github.com/uos/kurt_driver.git', version: hydro}
- git: {local-name: uos-ros-pkg/kurt_navigation, uri: 'https://github.com/uos/kurt_navigation.git', version: hydro}
- git: {local-name: uos-ros-pkg/uos_slam, uri: 'https://github.com/uos/uos_slam', version: hydro}
- git: {local-name: uos-ros-pkg/uos_tools, uri: 'https://github.com/uos/uos_tools', version: hydro}
- git: {local-name: uos-ros-pkg/volksbot_driver, uri: 'https://github.com/uos/volksbot_driver', version: hydro}
