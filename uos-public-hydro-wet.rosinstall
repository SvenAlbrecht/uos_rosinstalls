# This rosinstall contains all uos-ros-pkg stacks for ROS Hydro that are "wet" (i.e., based on catkin). It uses
# public (i.e. anonymous) git URLs, so no SSH keys are required. It is
# referenced from http://www.ros.org/wiki/uos-ros-pkg.

- git: {local-name: calvin_robot, uri: 'https://github.com/uos/calvin_robot', version: hydro_catkin}
- git: {local-name: diffdrive_gazebo_plugin, uri: 'https://github.com/uos/diffdrive_gazebo_plugin', version: hydro_catkin}
- git: {local-name: imu_tools, uri: 'https://github.com/ccny-ros-pkg/imu_tools', version: hydro}
- git: {local-name: katana_driver, uri: 'https://github.com/uos/katana_driver', version: hydro_catkin}
- git: {local-name: kurt_driver, uri: 'https://github.com/uos/kurt_driver.git', version: hydro_catkin}
- git: {local-name: phidgets_drivers, uri: 'https://github.com/ccny-ros-pkg/phidgets_drivers', version: hydro}
- git: {local-name: sick_tim, uri: 'https://github.com/uos/sick_tim', version: hydro_catkin}
- git: {local-name: uos_tools, uri: 'https://github.com/uos/uos_tools', version: hydro_catkin}
- git: {local-name: volksbot_driver, uri: 'https://github.com/uos/volksbot_driver', version: hydro_catkin}
